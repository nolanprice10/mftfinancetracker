{
  "expo": {
    "name": "MFT Finance Tracker",
    "slug": "mft-finance-tracker",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.mftfinancetracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mftfinancetracker.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "@supabase/supabase-js",
        {}
      ]
    ]
  }
}
